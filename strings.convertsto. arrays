    <body>
        <script>
            var m ="srikanth"
          for(var i=m.length-1;i>=0;i--){
            console.log(m[i])
          }

        var x="srikanth"
        for(var i=0;i<=x.length-1;i++){
            console.log(x[i])
        }
        </script>
    </body>
